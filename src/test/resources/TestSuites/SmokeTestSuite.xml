<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="SmokeTestSuite" verbose="1">

	<listeners>
		<listener
			class-name="com.herokuapp.theinternet.base.TestListener" />
	</listeners>

	<test name="PositiveLoginTests">
		<classes>
			<class
				name="com.herokuapp.theinternet.Loginpagetests.PositiveLoginTests" />
		</classes>
	</test>

	<test name="AlertTests">
		<classes>
			<class
				name="com.herokuapp.theinternet.alertpagetests.AlertTests" />
		</classes>
	</test>

	<test name="UploadTests">
		<classes>
			<class name="com.herokuapp.theinternet.uploadtests.UploadTests" />
		</classes>
	</test>

</suite>